<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="dashboard" stylesheets="@../../style/css/long-style.css"
            xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.gymmanagementsystem.controllers.main.DashboardMenuController">
    <VBox alignment="CENTER" layoutX="201.0" layoutY="190.0" prefHeight="60.0" prefWidth="101.0"
          AnchorPane.rightAnchor="898.0">
        <Text fill="#5e5b5b" strokeType="OUTSIDE" strokeWidth="0.0" text="Menu list">
            <font>
                <Font name="Helvetica" size="24.0"/>
            </font>
        </Text>
    </VBox>
    <HBox alignment="CENTER_LEFT" layoutX="201.0" layoutY="343.0" onMouseClicked="#registrationMenuHandler"
          prefHeight="68.0" prefWidth="344.0" styleClass="menu-center" AnchorPane.rightAnchor="655.0">
        <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onMouseClicked="#registrationMenuHandler"
                   prefHeight="59.0" prefWidth="75.0" style="-fx-background-color: #3a6188#3a6188;" text=".">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../../style/icons/icons8-registration-30.png"/>
                </ImageView>
            </graphic>
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </JFXButton>
        <VBox prefHeight="59.0" prefWidth="271.0" spacing="2.0">
            <Text fill="#464444" strokeType="OUTSIDE" strokeWidth="0.0" text="Registrations">
                <font>
                    <Font name="Arial Bold" size="19.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0" top="10.0"/>
                </VBox.margin>
            </Text>
            <Text fill="#575353" strokeType="OUTSIDE" strokeWidth="0.0" text="add new customer">
                <font>
                    <Font name="Helvetica" size="17.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0"/>
                </VBox.margin>
            </Text>
            <HBox.margin>
                <Insets left="3.0"/>
            </HBox.margin>
        </VBox>
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="201.0" layoutY="250.0" onMouseClicked="#homeMenuHandler" prefHeight="68.0"
          prefWidth="355.0" styleClass="menu-center" AnchorPane.rightAnchor="644.0">
        <StackPane>
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
            <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onMouseClicked="#homeMenuHandler"
                       prefHeight="59.0" prefWidth="75.0" style="-fx-background-color: #3a6188#3a6188;" text=".">
                <graphic>
                    <ImageView fitHeight="32.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../../style/icons/icons8-home-24.png"/>
                    </ImageView>
                </graphic>
            </JFXButton>
        </StackPane>
        <VBox prefHeight="59.0" prefWidth="271.0" spacing="2.0">
            <Text fill="#464444" strokeType="OUTSIDE" strokeWidth="0.0" text="Home">
                <font>
                    <Font name="Arial Bold" size="19.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0" top="10.0"/>
                </VBox.margin>
            </Text>
            <Text fill="#575353" strokeType="OUTSIDE" strokeWidth="0.0" text="Where you can search customers">
                <font>
                    <Font name="Helvetica" size="17.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0"/>
                </VBox.margin>
            </Text>
        </VBox>
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="605.0" layoutY="256.0" onMouseClicked="#outDatedMenuHandler"
          prefHeight="68.0" prefWidth="344.0" styleClass="menu-center" AnchorPane.rightAnchor="251.0">
        <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onMouseClicked="#outDatedMenuHandler"
                   prefHeight="59.0" prefWidth="75.0" style="-fx-background-color: #3a6188#3a6188;" text=".">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../../style/icons/icons8-user-30.png"/>
                </ImageView>
            </graphic>
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </JFXButton>
        <VBox prefHeight="59.0" prefWidth="271.0" spacing="2.0">
            <Text fill="#464444" strokeType="OUTSIDE" strokeWidth="0.0" text="Out dated">
                <font>
                    <Font name="Arial Bold" size="19.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0" top="10.0"/>
                </VBox.margin>
            </Text>
            <Text fill="#575353" strokeType="OUTSIDE" strokeWidth="0.0" text="members go beyond the due date">
                <font>
                    <Font name="Helvetica" size="17.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0"/>
                </VBox.margin>
            </Text>
        </VBox>
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="201.0" layoutY="432.0" onMouseClicked="#logOutMenuHandler" prefHeight="68.0"
          prefWidth="344.0" styleClass="menu-center" AnchorPane.rightAnchor="655.0">
        <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onMouseClicked="#logOutMenuHandler"
                   prefHeight="59.0" prefWidth="75.0" style="-fx-background-color: #3a6188#3a6188;" text=".">
            <graphic>
                <ImageView fitHeight="36.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../../style/icons/icons8-reset-48.png"/>
                </ImageView>
            </graphic>
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </JFXButton>
        <VBox prefHeight="59.0" prefWidth="271.0" spacing="2.0">
            <Text fill="#464444" strokeType="OUTSIDE" strokeWidth="0.0" text="Logout">
                <font>
                    <Font name="Arial Bold" size="19.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0" top="10.0"/>
                </VBox.margin>
            </Text>
            <Text fill="#575353" strokeType="OUTSIDE" strokeWidth="0.0" text="back to login page">
                <font>
                    <Font name="Helvetica" size="17.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0"/>
                </VBox.margin>
            </Text>
        </VBox>
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="605.0" layoutY="352.0" onMouseClicked="#reportMenuHandler" prefHeight="68.0"
          prefWidth="344.0" styleClass="menu-center" AnchorPane.rightAnchor="251.0">
        <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onMouseClicked="#reportMenuHandler"
                   prefHeight="59.0" prefWidth="75.0" style="-fx-background-color: #3a6188#3a6188;" text=".">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../../style/icons/icons8-business-report-50.png"/>
                </ImageView>
            </graphic>
            <HBox.margin>
                <Insets left="10.0"/>
            </HBox.margin>
        </JFXButton>
        <VBox prefHeight="59.0" prefWidth="271.0" spacing="2.0">
            <Text fill="#464444" strokeType="OUTSIDE" strokeWidth="0.0" text="Reports">
                <font>
                    <Font name="Arial Bold" size="19.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0" top="10.0"/>
                </VBox.margin>
            </Text>
            <Text fill="#575353" strokeType="OUTSIDE" strokeWidth="0.0" text="get reports from here">
                <font>
                    <Font name="Helvetica" size="17.0"/>
                </font>
                <VBox.margin>
                    <Insets left="10.0"/>
                </VBox.margin>
            </Text>
        </VBox>
    </HBox>
</AnchorPane>
